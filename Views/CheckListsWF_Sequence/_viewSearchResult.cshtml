@model QA.Entities.Business_Entities.Ent_CheckListFlow_Sequence

<script src="~/Design_Template/build/js/custom.js"></script>

<div class="modal-dialog modal-lg">
    <div class="modal-content">
        <div class="modal-header">
            <h4 class="modal-title" id="myModalLabel">@Localization.CheckListFlow_Sequence.SequenceDetails</h4>
            <button type="button" class="close" data-dismiss="modal">
                <span aria-hidden="true">×</span>
            </button>
        </div>

        <!-- Modal Body -->
        <div class="modal-body">
            <div class="x_content">
                <div class="form-group">
                    <div class="row">
                        @Html.LabelFor(model => model.name, Localization.CheckListFlow_Sequence.name_Search
         , htmlAttributes: new { @class = "control-label col-md-3 col-sm-3 col-12" })

                        <div class="col-md-6 col-sm-6 col-12">
                            @Html.EditorFor(model => model.name, new
                       {
                           htmlAttributes = new
                           {
                               @class = "form-control col-md-7 col-12",
                               @disabled = "disabled",
                               @readonly = "readonly"
                           }
                       })
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <div class="row">
                        @Html.LabelFor(model => model.description, htmlAttributes: new { @class = "control-label col-md-3 col-sm-3 col-12" })
                        <div class="col-md-6 col-sm-6 col-12">
                            @Html.TextAreaFor(model => model.description, 1, 0, htmlAttributes: new
                       {
                           @class = "form-control col-md-7 col-12",
                           @disabled = "disabled",
                           @readonly = "readonly"
                       })
                            <br />
                        </div>
                    </div>
                </div>

                <div class="form-group">
                    <div class="row">
                        @Html.LabelFor(model => model.technicianID
         , htmlAttributes: new { @class = "control-label col-md-3 col-sm-3 col-12" })

                        <div class="col-md-6 col-sm-6 col-12">
                            @Html.EditorFor(model => model.technicianName, new
                       {
                           htmlAttributes = new
                           {
                               @class = "form-control col-md-7 col-12",
                               @disabled = "disabled",
                               @readonly = "readonly"
                           }
                       })
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <div class="row">
                        @Html.LabelFor(model => model.technician_maxDays
                            , htmlAttributes: new { @class = "control-label col-md-3 col-sm-3 col-12" })
                        <div class="col-md-6 col-sm-6 col-12">
                            @Html.EditorFor(model => model.technician_maxDays, new
                            {
                           htmlAttributes = new
                                {
                               @class = "form-control col-md-7 col-12",
                               @type = "number",
                               @disabled = "disabled",
                               @readonly = "readonly"
                           }
                       })
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <div class="row">
                        @Html.LabelFor(model => model.supervisorEngID
                            , htmlAttributes: new { @class = "control-label col-md-3 col-sm-3 col-12" })
                        <div class="col-md-6 col-sm-6 col-12">
                            @Html.EditorFor(model => model.supervisorEngName, new
                            {
                           htmlAttributes = new
                           {
                               @class = "form-control col-md-7 col-12",
                               @disabled = "disabled",
                               @readonly = "readonly"
                           }
                       })
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <div class="row">
                        @Html.LabelFor(model => model.superEng_maxDays
                            , htmlAttributes: new { @class = "control-label col-md-3 col-sm-3 col-12" })
                        <div class="col-md-6 col-sm-6 col-12">
                            @Html.EditorFor(model => model.superEng_maxDays, new
                            {
                                htmlAttributes = new
                                {
                                             @class = "form-control col-md-7 col-12",
                                             @type = "number",
                                             @disabled = "disabled",
                                             @readonly = "readonly"
                                         }
                                     })
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <div class="row">
                        @Html.LabelFor(model => model.qALabID
                            , htmlAttributes: new { @class = "control-label col-md-3 col-sm-3 col-12" })
                        <div class="col-md-6 col-sm-6 col-12">
                            @Html.EditorFor(model => model.qALabName, new
                            {
                                htmlAttributes = new
                                {
                                    @class = "form-control col-md-7 col-12",
                                    @disabled = "disabled",
                                    @readonly = "readonly"
                                }
                            })
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <div class="row">
                        @Html.LabelFor(model => model.qALab_maxDays
                            , htmlAttributes: new { @class = "control-label col-md-3 col-sm-3 col-12" })
                        <div class="col-md-6 col-sm-6 col-12">
                            @Html.EditorFor(model => model.qALab_maxDays
                                , new
                                     {
                                         htmlAttributes = new
                                         {
                                             @class = "form-control col-md-7 col-12",
                                             @type = "number",
                                             @disabled = "disabled",
                                             @readonly = "readonly"
                                         }
                                     })
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <div class="row">
                        @Html.LabelFor(model => model.representitiveSuperID
                            , htmlAttributes: new { @class = "control-label col-md-3 col-sm-3 col-12" })
                        <div class="col-md-6 col-sm-6 col-12">
                            @Html.EditorFor(model => model.representitiveSuperName, new
                            {
                                htmlAttributes = new
                                {
                                    @class = "form-control col-md-7 col-12",
                                    @disabled = "disabled",
                                    @readonly = "readonly"
                                }
                            })
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <div class="row">
                        @Html.LabelFor(model => model.repSuper_maxDays
                            , htmlAttributes: new { @class = "control-label col-md-3 col-sm-3 col-12" })
                        <div class="col-md-6 col-sm-6 col-12">
                            @Html.EditorFor(model => model.repSuper_maxDays
                                , new
                                     {
                                         htmlAttributes = new
                                         {
                                             @class = "form-control col-md-7 col-12",
                                             @type = "number",
                                             @disabled = "disabled",
                                             @readonly = "readonly"
                                         }
                                     })
                        </div>
                    </div>
                </div>

                <div class="form-group">
                    <div class="row">
                        @Html.LabelFor(model => model.isActive, htmlAttributes: new { @class = "control-label col-md-3 col-sm-3 col-12" })
                        <div class="col-md-9 col-sm-9 col-12">
                            <div class="">
                                <label>
                                    @Html.CheckBoxFor(model => model.isActive, htmlAttributes: new
                                    {
                                        @class = "js-switch",
                                        @checked = "checked",
                                        @disabled = "disabled"
                                    })
                                </label>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- End Modal Body -->

        <div class="modal-footer">
            <button type="button" class="btn btn-light" data-dismiss="modal">
                @Localization.Global.BtnOk
            </button>
        </div>
    </div>
</div>

