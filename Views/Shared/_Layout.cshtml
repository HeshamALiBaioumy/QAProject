    <script src="~/Design_Template/vendors/nprogress/nprogress.js"></script>
    <!-- iCheck -->
    <script src="~/Design_Template/vendors/iCheck/icheck.min.js"></script>
    <!-- bootstrap-daterangepicker -->
    <script src="~/Design_Template/vendors/moment/min/moment.min.js"></script>
    <script src="~/Design_Template/vendors/bootstrap-daterangepicker/daterangepicker.js"></script>

    <!-- bootstrap-datepicker -->
    <script src="~/Design_Template/vendors/bootstrap-datepicker/js/bootstrap-datepicker.js"></script>


    <!--  Colorpicker -->
    <script src="~/Design_Template/vendors/farbelous-bootstrap-colorpicker/js/bootstrap-colorpicker.min.js"></script>
