    <script src="~/Design_Template/vendors/jquery.tagsinput/src/jquery.tagsinput.js"></script>
    <!-- Switchery -->
    <script src="~/Design_Template/vendors/switchery/dist/switchery.min.js"></script>
    <!-- Select2 -->
    <script src="~/Design_Template/vendors/select2/dist/js/select2.full.min.js"></script>
    <!-- Parsley -->
    <script src="~/Design_Template/vendors/parsleyjs/dist/parsley.min.js"></script>
    <!-- Autosize -->
    <script src="~/Design_Template/vendors/autosize/dist/autosize.min.js"></script>
    <!-- jQuery autocomplete -->
    <script src="~/Design_Template/vendors/devbridge-autocomplete/dist/jquery.autocomplete.min.js"></script>
    <!-- starrr -->
    <script src="~/Design_Template/vendors/starrr/dist/starrr.js"></script>

    <!-- PNotify -->
    <script src="~/Design_Template/vendors/pnotify/dist/pnotify.js"></script>
    <script src="~/Design_Template/vendors/pnotify/dist/pnotify.buttons.js"></script>
    <script src="~/Design_Template/vendors/pnotify/dist/pnotify.nonblock.js"></script>

    <!-- Datatables -->
    <script src="~/Design_Template/vendors/datatables.net/js/jquery.dataTables.min.js"></script>
    <script src="~/Design_Template/vendors/datatables.net-bs/js/dataTables.bootstrap4.min.js"></script>
    <script src="~/Design_Template/vendors/datatables.net-buttons/js/dataTables.buttons.min.js"></script>
    <script src="~/Design_Template/vendors/datatables.net-buttons-bs/js/buttons.bootstrap4.min.js"></script>
    <script src="~/Design_Template/vendors/datatables.net-buttons/js/buttons.flash.min.js"></script>
    <script src="~/Design_Template/vendors/pdfmake/build/pdfmake.min.js"></script>
    <script src="~/Design_Template/vendors/datatables.net-buttons/js/buttons.html5.min.js"></script>
    <script src="~/Design_Template/vendors/datatables.net-buttons/js/buttons.print.min.js"></script>
    <script src="~/Design_Template/vendors/datatables.net-fixedheader/js/dataTables.fixedHeader.min.js"></script>
    <script src="~/Design_Template/vendors/datatables.net-keytable/js/dataTables.keyTable.min.js"></script>
    <script src="~/Design_Template/vendors/datatables.net-responsive/js/dataTables.responsive.min.js"></script>
    <script src="~/Design_Template/vendors/datatables.net-responsive-bs/js/responsive.bootstrap4.min.js"></script>
    <script src="~/Design_Template/vendors/datatables.net-scroller/js/dataTables.scroller.min.js"></script>
    <script src="~/Design_Template/vendors/jszip/dist/jszip.min.js"></script>
    <script src="~/Design_Template/vendors/pdfmake/build/vfs_fonts.js"></script>

    <script src="~/Design_Template/Scripts/jquery.validate.min.js"></script>
    <script src="~/Design_Template/Scripts/jquery.validate.unobtrusive.js"></script>

    <script src="~/Design_Template/Scripts/jquery.unobtrusive-ajax.js"></script>

    <!-- bootstrap-progressbar -->
    <script src="~/Design_Template/vendors/bootstrap-progressbar/bootstrap-progressbar.min.js"></script>

    <!-- jQuery Navigation -->
    <script src="~/Design_Template/build/js/navigition.js"></script>

    <!-- Custom Theme Scripts -->
    <script src="~/Design_Template/build/js/custom.js"></script>

    <!-- Custom Class For Direction Css Scripts -->
    <script>
        $('.datetype').removeAttr("data-val-date");

        $(document).ready(function () {
            $("html[lang=ar]").attr("dir", "rtl")
                .find("body").addClass("right-to-left");
        });
    </script>

    <!-- jQuery Smart Wizard -->
    <script src="~/Design_Template/vendors/jQuery-Smart-Wizard/js/jquery.smartWizard.js"></script>

    <!-- Popper -->
    <script src="~/Design_Template/vendors/popper/popper.min.js"></script>

    <!-- jQuery Knob -->
    <script src="~/Design_Template/vendors/jquery-knob/dist/jquery.knob.min.js"></script>

    <!--                   Dashboard Scripts          -->
    <!-- DateJS -->
    <script src="~/Design_Template/vendors/DateJS/build/date.js"></script>

    <!-- Chart.js -->
    <script src="~/Design_Template/vendors/Chart.js/dist/Chart.min.js"></script>
    <!-- gauge.js -->
    <script src="~/Design_Template/vendors/gauge.js/dist/gauge.min.js"></script>

    <!-- Skycons -->
    <script src="~/Design_Template/vendors/skycons/skycons.js"></script>
    <!-- Flot -->
    <script src="~/Design_Template/vendors/Flot/jquery.flot.js"></script>
    <script src="~/Design_Template/vendors/Flot/jquery.flot.pie.js"></script>
    <script src="~/Design_Template/vendors/Flot/jquery.flot.time.js"></script>
    <script src="~/Design_Template/vendors/Flot/jquery.flot.stack.js"></script>
    <script src="~/Design_Template/vendors/Flot/jquery.flot.resize.js"></script>
    <!-- Flot plugins -->
    <script src="~/Design_Template/vendors/flot.orderbars/js/jquery.flot.orderBars.js"></script>
    <script src="~/Design_Template/vendors/flot-spline/js/jquery.flot.spline.min.js"></script>
    <script src="~/Design_Template/vendors/flot.curvedlines/curvedLines.js"></script>

    <!-- ECharts -->
    <script src="~/Design_Template/vendors/echarts/dist/echarts.min.js"></script>
    <script src="~/Design_Template/vendors/echarts/map/js/world.js"></script>

    @RenderSection("scripts", required: false)
